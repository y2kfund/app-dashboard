    "@y2kfund/core": "git+https://github.com/y2kfund/app-core.git",
    "@y2kfund/positions": "git+https://github.com/y2kfund/app-positions.git",
    "@y2kfund/summary": "git+https://github.com/y2kfund/app-summary.git",
    "@y2kfund/tasks": "git+https://github.com/y2kfund/app-tasks.git",
    "@y2kfund/trades": "git+https://github.com/y2kfund/app-trades.git",
    "@y2kfund/transfers": "git+https://github.com/y2kfund/app-transfers.git",
    "@y2kfund/thesis": "git+https://github.com/y2kfund/app-thesis.git",
    "@y2kfund/cash-transactions": "git+https://github.com/y2kfund/app-cash-transactions.git",
    "@y2kfund/put-positions-for-single-instrument": "git+https://github.com/y2kfund/app-put-positions-for-single-instrument.git",
