    "gridstack": "^12.3.3",
    "html2canvas": "^1.4.1",
    "vue": "^3.4.0",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.4.0",
    "patch-package": "^8.0.1",
    "vite": "^5.0.0",
    "vue-tsc": "^2.0.0"
  },
  "scripts": {
