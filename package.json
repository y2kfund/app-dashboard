    "deps:dev": "npm install @y2kfund/analyze-chat@file:../app-analyze-chat @y2kfund/analyze-timeline@file:../app-analyze-timeline @y2kfund/positions@file:../app-positions @y2kfund/summary@file:../app-summary @y2kfund/thesis@file:../app-thesis @y2kfund/tasks@file:../app-tasks @y2kfund/trades@file:../app-trades @y2kfund/transfers@file:../app-transfers @y2kfund/cash-transactions@file:../app-cash-transactions @y2kfund/put-positions-for-single-instrument@file:../app-put-positions-for-single-instrument @y2kfund/current-positions-for-single-instrument@file:../app-current-position-across-all-accounts-for-single-instrument @y2kfund/call-positions-for-single-instrument@file:../app-call-positions-for-single-instrument @y2kfund/ai-recommendations-for-single-instrument@file:../app-ai-recommendations-for-single-instrument --no-save",
