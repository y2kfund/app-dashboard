{
  "name": "app-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "_devNotes": {
    "npm run dev": "uses local packages from ../app-positions and ../app-summary",
    "npm run build": "uses packages from Git repository for app-positions and app-summary",
    "dependencies": "The dependencies below are PRODUCTION versions. These are used when npm install is run by cloudflare"
  },
  "dependencies": {
    "@y2kfund/analyze-chat": "git+https://github.com/y2kfund/app-analyze-chat.git",
    "@y2kfund/analyze-timeline": "git+https://github.com/y2kfund/app-analyze-timeline.git",
    "@y2kfund/core": "git+https://github.com/y2kfund/app-core.git",
    "@y2kfund/summary": "git+https://github.com/y2kfund/app-summary.git",
    "@y2kfund/positions": "git+https://github.com/y2kfund/app-positions.git",
    "html2canvas": "^1.4.1",
    "vue": "^3.4.0",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vue-tsc": "^2.0.0"
  },
  "scripts": {
    "dev": "npm run deps:dev && vite",
    "build": "npm run deps:prod && vite build",
    "preview": "vite preview --port 5173",
    "_comment": "deps:dev installs local packages temporarily (--no-save), deps:prod ensures Git repos are used",
    "deps:dev": "npm install @y2kfund/analyze-chat@file:../app-analyze-chat @y2kfund/analyze-timeline@file:../app-analyze-timeline @y2kfund/positions@file:../app-positions @y2kfund/summary@file:../app-summary --no-save",
    "deps:prod": "npm install @y2kfund/analyze-chat@git+https://github.com/y2kfund/app-analyze-chat.git @y2kfund/analyze-timeline@git+https://github.com/y2kfund/app-analyze-timeline.git @y2kfund/positions@git+https://github.com/y2kfund/app-positions.git @y2kfund/summary@git+https://github.com/y2kfund/app-summary.git --no-save"
  }
}
