{
  "name": "@y2kfund/app-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "_devNotes": {
    "npm run dev": "uses local packages from ../app-positions and ../app-summary",
    "npm run build": "uses packages from Git repository for app-positions and app-summary",
    "dependencies": "The dependencies below are PRODUCTION versions. These are used when npm install is run by cloudflare"
  },
